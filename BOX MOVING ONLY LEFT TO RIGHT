<!DOCTYPE html>
<html lang="en">
<head>
<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<title>Moving Rock</title>
<style>
  #rock {
    position: absolute;
    left: 0;
    top: 50%;
    transform: translate(0, -50%);
    width: 100px;
    height: 100px;
    background-color: gray;
  }
</style>
</head>
<body>
  <div id="rock"></div>
  <script>
    function moveRock() {
      const rock = document.getElementById('rock');
      let position = 0;
      const interval = setInterval(() => {
        position += 1;
        rock.style.left = position + 'px';
        if (position >= window.innerWidth - 100) {
          clearInterval(interval);
        }
      }, 50);
    }
    setTimeout(moveRock, 5000);
  </script>
</body>
</html>
